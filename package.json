{
  "name": "json-s-generator",
  "version": "0.0.5",
  "description": "JSON schema generator writen in TypeScript supporting from JSON Schema draft-07.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mowgliLab/json-s-gen.git"
  },
  "keywords": [
    "JSON",
    "schema",
    "generator",
    "JSON schema",
    "draft-07",
    "typescript"
  ],
  "author": "Alain Daussin <mowglilab@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mowgliLab/json-s-gen/issues"
  },
  "homepage": "https://github.com/mowgliLab/json-s-gen#readme",
  "devDependencies": {
    "@types/chai": "~4.1.4",
    "@types/lodash": "~4.14.110",
    "@types/mocha": "~5.2.4",
    "chai": "~4.1.2",
    "coveralls": "^3.0.2",
    "mocha": "~5.2.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.6",
    "ts-node": "~7.0.0",
    "typescript": "~2.9.2"
  },
  "dependencies": {
    "lodash": "~4.17.10"
  }
}
